.login_forms
  %h2 Log in
  = form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: 'form-group' }) do |f|
    = f.label :email
    = f.email_field :email, autofocus: true
    = f.label :password
    = f.password_field :password, autocomplete: "off"
    - if devise_mapping.rememberable?
      = f.label :remember_me
      = f.check_box :remember_me
    .actions
      = f.submit "Log in"
  = render "devise/shared/links"
